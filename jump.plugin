#!name = Jump Ad Blocker
#!desc = Block Jump App opening ads
#!author = Yui
#!homepage = https://raw.githubusercontent.com/kitoyui/loon/main/jump.plugin
#!version = 1.0

[Script]
http-request ^https?:\/\/.*jumpapp.*\/ads\/.* script-response-body reject-ad.js
http-response ^https?:\/\/.*jumpapp.*\/ads\/.* requires-body=1,script-response-body reject-ad.js

[MITM]
hostname = *.jumpapp.*

[ScriptContent]
reject-ad.js:
  $done({ response: { status: 200, body: "" } });